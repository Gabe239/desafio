<div>
  <h1>Lista de productos</h1>
  {{#if products}}
  <ul>
    {{#each products}}
    <li>
      <p>Title: {{this.title}}</p>
      <p>Description: {{this.description}}</p>
      <p>Price: {{this.price}}</p>
      <p>Thumbnail: {{this.thumbnail}}</p>
      <p>Code: {{this.code}}</p>
      <p>Stock: {{this.stock}}</p>
      <p>Id: {{this.id}}</p>
    </li>
    {{/each}}
  </ul>
  {{else}}
  <p>No hay productos disponibles.</p>
  {{/if}}
</div>
